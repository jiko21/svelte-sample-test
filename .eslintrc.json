{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "svelte3",
        "@typescript-eslint"
    ],
    "settings": {
        "svelte3/typescript": true
    },
    "overrides": [
        {
            "files": ["*.svelte"],
            "processor": "svelte3/svelte3"
        }
    ],
    "rules": {
    }
}
